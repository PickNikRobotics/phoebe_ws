<?xml version="1.0" encoding="UTF-8" ?>
<root BTCPP_format="4" main_tree_to_execute="Multi-tip Path Move With Pose">
  <BehaviorTree ID="Multi-tip Path Move With Pose" _description="">
    <Control ID="Sequence">
      <Control ID="Sequence">
        <Action
          ID="CreateStampedPose"
          reference_frame="left_grasp_link"
          stamped_pose="{init_stamped_pose}"
        />
        <Action
          ID="AddPoseStampedToVector"
          input="{init_stamped_pose}"
          vector="{pose_stamped_vector}"
        />
        <Action
          ID="AddPoseStampedToVector"
          input="{stamped_pose}"
          vector="{pose_stamped_vector}"
        />
      </Control>
      <Control ID="Sequence">
        <Action
          ID="PlanCartesianPath"
          acceleration_scale_factor="1.000000"
          blending_radius="0.020000"
          debug_solution="{debug_solution}"
          ik_cartesian_space_density="0.010000"
          ik_joint_space_density="0.100000"
          joint_trajectory_msg="{joint_trajectory_msg}"
          path="{pose_stamped_vector}"
          position_only="false"
          tip_links="left_grasp_link;right_grasp_link"
          trajectory_sampling_rate="100"
          velocity_scale_factor="1.000000"
          max_optimizer_iterations="1"
          tip_offset="0;0;0;0;0;0"
          planning_group_name="arms_only"
        />
        <Action
          ID="WaitForUserTrajectoryApproval"
          solution="{debug_solution}"
          cartesian_path_links="left_grasp_link"
          _skipIf="true"
        />
      </Control>
      <Action
        ID="SwitchController"
        activate_controllers="joint_trajectory_controller"
      />
      <Action
        ID="ExecuteTrajectory"
        execution_pipeline="jtc"
        controller_action_name="/joint_trajectory_controller/follow_joint_trajectory"
        goal_duration_tolerance="-1.000000"
        goal_position_tolerance="0.000000"
        joint_trajectory_msg="{joint_trajectory_msg}"
      />
    </Control>
  </BehaviorTree>
  <TreeNodesModel>
    <SubTree ID="Multi-tip Path Move With Pose">
      <MetadataFields>
        <Metadata runnable="true" />
        <Metadata subcategory="Application - Advanced Examples" />
      </MetadataFields>
      <inout_port
        name="stamped_pose"
        default="{stamped_pose}"
        type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"
      >
        moves both arms with the given transform(trajectory relative to left
        arm)
      </inout_port>
    </SubTree>
  </TreeNodesModel>
</root>
