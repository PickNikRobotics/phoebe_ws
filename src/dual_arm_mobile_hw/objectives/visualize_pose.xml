<?xml version="1.0" encoding="UTF-8" ?>
<root BTCPP_format="4" main_tree_to_execute="Visualize Pose">
  <!--//////////-->
  <BehaviorTree ID="Visualize Pose" _description="" _favorite="false">
    <Control ID="Sequence" name="TopLevelSequence">
      <Action
        ID="CreateStampedPose"
        reference_frame="left_grasp_link"
        stamped_pose="{stamped_pose}"
      />
      <Action
        ID="TransformPose"
        input_pose="{stamped_pose}"
        output_pose="{transformed_pose}"
        translation_xyz="-0.000;0.013;0.094"
        quaternion_xyzw="0.0;0.906;-0.423;0.0"
      />
      <Action
        ID="VisualizePose"
        marker_lifetime="0.000000"
        marker_name="transformed_pose"
        marker_size="0.100000"
        pose="{transformed_pose}"
      />
    </Control>
  </BehaviorTree>
  <TreeNodesModel>
    <SubTree ID="Visualize Pose">
      <MetadataFields>
        <Metadata runnable="true" />
        <Metadata subcategory="Perception - Visualization" />
      </MetadataFields>
    </SubTree>
  </TreeNodesModel>
</root>
